<environment include="Development">
    <link rel="preload" as="style" href="~/lib/bootstrap/3.3.7/css/bootstrap.min.css" />
    <link rel="preload" as="style" href="~/lib/bootstrap/3.3.7/css/bootstrap-theme.min.css" />
